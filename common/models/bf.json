    },
    "created": {
      "type": "date"
    },
    "modified": {
      "type": "date"
    },
    "bfdata": {
      "type": "object"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
