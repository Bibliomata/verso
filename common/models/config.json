{
  "name": "config",
  "description": "Generic model for storing configuration data as JSON",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "ReadOnly": {
      "id": true,
      "metadata": true
    }
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "defaultFn": "uuidv4"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "configType": {
      "type": "string",
      "required": true
    },
    "json": {
      "type": "object",
      "required": true
    },
    "metadata": {
      "type": {
        "createDate": "date",
        "updateDate": "date",
        "createUser": "string",
        "updateUser": "string"
      }
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "profile_editor",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
